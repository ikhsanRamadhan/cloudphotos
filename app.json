{
  "expo": {
    "name": "cloudphotos",
    "slug": "cloudphotos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#181818"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-icon.png",
        "backgroundColor": "#181818"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nashki.cloudphotos",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "ACCESS_MEDIA_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDb-X_OrxH7PFx3_7KU0aQXHdsjVLsWWbI"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-video"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4e55a2e5-7929-4303-9792-acaedc342d7e"
      }
    }
  }
}
